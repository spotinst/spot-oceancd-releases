{
  "apiVersion": "spot.io/v1beta1",
  "kind": "SpotDeployment",
  "metadata": {
    "name": "spotdeployment-headers-demo",
    "labels": {
      "app": "rollout-headers-test"
    }
  },
  "spec": {
    "replicas": 2,
    "selector": {
      "matchLabels": {
        "app": "rollout-headers-test"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "rollout-headers-test"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "rollout-headers-test",
            "image": "argoproj/rollouts-demo:red",
            "ports": [
              {
                "name": "http",
                "containerPort": 8080,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
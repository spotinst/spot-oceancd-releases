{
  "rolloutSpec": {
    "name": "service-rolloutspec",
    "spotDeployment": {
      "clusterId": "string",
      "namespace": "string",
      "name": "string"
    },
    "strategy": {
      "name": "service-rolloutspec",
      "args": [
        {
          "name": "app",
          "value": "service",
          "valueFrom": {
            "secretRef": {
              "name": "app-name",
              "key": "app"
            },
            "fieldRef": {
              "fieldPath": "metadata.labels['app']"
            }
          }
        }
      ]
    },
    "traffic": {
      "canaryService": "canary",
      "stableService": "stable"
    },
    "failurePolicy": {
      "action": "abort"
    }
  }
}
